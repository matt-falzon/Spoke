<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="Spokesman.MainPage"
    xmlns:local="clr-namespace:Spokesman">
    <local:HomePage Title="Home" Icon="icon_wheel.gif"/>
    <NavigationPage Title="Results" Icon="icon_wheel.gif">
        <x:Arguments>
            <local:ResultsPage />
        </x:Arguments>
    </NavigationPage>
    <local:GroupsPage Title="Groups" Icon="icon_wheel.gif"/>
    <NavigationPage Title="Notification" Icon="icon_wheel.gif">
        <x:Arguments>
            <local:NotificationsPage />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>
