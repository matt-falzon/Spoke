<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="Spokesman.MainPage"
    xmlns:local="clr-namespace:Spokesman">
    <local:HomePage Title="Home" />
    <NavigationPage Title="Results" >
        <x:Arguments>
            <local:ResultsPage />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Policy"  >
        <x:Arguments>
            <local:PolicyPage />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Notification">
        <x:Arguments>
            <local:NotificationsPage />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>
